{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@Components/*": [
        "./src/components/*"
      ],
      "@Pages/*": [
        "./src/pages/*"
      ],
      "@Styles/*": [
        "./src/styles/*"
      ],
      "@Context/*": [
        "./src/context/*"
      ],
      "@Services/*": [
        "./src/services/*"
      ],
      "@Utils/*": [
        "./src/utils/*"
      ],
      "@Routes/*": [
        "./src/routes/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "build"
  ]
}